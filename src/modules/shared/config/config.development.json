{
  "API_URL": "https://development-app.com",
  "app": {
    "icon": {
      "light": "/icons/app-icon.svg",
      "dark": "/icons/app-icon.svg"
    },
    "name": "Vite"
  },
  "settings": {
    "defaultLang": "en",
    "defaultFontSize": "M",
    "defaultTheme": "light"
  },
  "dev": true,
  "themes": {
    "light": {
      "background": {
        "main": "#FFFFFF",
        "topbar": "#FFFFFF",
        "activeTab": "#F8FAFC",
        "sidebar": "#FBFBFB"
      },
      "text": {
        "primary": "#263779",
        "secondary": "#B4BAD3",
        "disabled": "#9497A7",
        "button": "#FFFFFF"
      },
      "button": {
        "color": "#263779",
        "hover": "#0C2761",
        "disabled": "#0C2761"
      },
      "input": {
        "primary": "#F2F2F2",
        "default": "#FFFFFF"
      },
      "border": {
        "primary": "#E1E7F2",
        "secondary": "#D4D7E4"
      }
    },
    "dark": {
      "background": {
        "main": "#000000",
        "topbar": "#000000",
        "activeTab": "#17151C",
        "sidebar": "#000000"
      },
      "text": {
        "primary": "#D0D0D0",
        "secondary": "#9A9A9A",
        "disabled": "#9A9A9A",
        "button": "#000000"
      },
      "button": {
        "color": "#FFFFFF",
        "hover": "#D7D7D7",
        "disabled": "#ACACAC"
      },
      "input": {
        "primary": "#D0D0D0",
        "default": "#F6F6F6"
      },
      "border": {
        "primary": "#232323",
        "secondary": "#090909"
      }
    }
